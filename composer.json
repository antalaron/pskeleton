{
    "name": "pskeleton/pskeleton",
    "description": "Project creator from skeleton",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Antal Áron",
            "email": "antalaron@antalaron.hu"
        }
    ],
    "autoload": {
        "psr-4": {
            "PSkeleton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\PSkeleton\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "^3.3",
        "guzzlehttp/guzzle": "^6.3",
        "raulfraile/distill": "~0.9,!=0.9.3,!=0.9.4",
        "symfony/filesystem": "^3.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.3"
    },
    "bin": [
        "bin/pskeleton"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
